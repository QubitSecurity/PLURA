{
  "manifest_version": 3,
  "name": "VirusTotal Search",
  "version": "1.0",
  "description": "Search for the selected text on VirusTotal from the right-click menu.",
  "permissions": [
    "contextMenus",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
